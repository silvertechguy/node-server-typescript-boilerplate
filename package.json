{
  "name": "node-server-typescript-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts",
    "dev2": "nodemon --exec ts-node src/index.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/mongoose": "^5.10.1",
    "gen-env-types": "^1.0.4",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.15"
  }
}
